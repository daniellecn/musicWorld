// Place your settings in this file to overwrite default and user settings.
{
    "window.zoomLevel": 1,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        // "**/*.js": true,
        "**/*.js.map": true,
        "**/*.spec.ts": true,
        ".history": true,
        "**/*.history": true
    },
    "typescript.referencesCodeLens.enabled": true,
    "mocha.enabled": false
}